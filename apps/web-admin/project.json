{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "web-admin",
  "projectType": "application",
  "sourceRoot": "apps/web-admin/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/.next"],
      "options": {
        "command": "next build",
        "cwd": "apps/web-admin",
        "forwardAllArgs": true
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev",
        "cwd": "apps/web-admin",
        "forwardAllArgs": true
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "next start",
        "cwd": "apps/web-admin",
        "forwardAllArgs": true
      }
    }
  },
  "tags": []
}
